import java.util.Scanner;


class Solution {

  public static void main(String[]args)  {
Scanner input = new Scanner (System.in);

System.out.println("X= ");
int x = input.nextInt();
System.out.println("output:  "+ x);
  }

    public int mySqrt(int x) {
        if (x<0)
            System.out.println("Error");
         else
          x = (int)Math.sqrt(x);

        return x;
        
    }
}



